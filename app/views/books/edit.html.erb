<h1>Editing Book</h1>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
  <label for="book_title">Title</label>
  <%= f.text_field :title %>
<%= f.label :Body %>
    <%= f.text_area :body %>
  <br></br>
  <%= f.submit 'Update Book' %>
  <br></br>
  <%= link_to "Show", book_path(@book) %> | <%= link_to "Back", books_path(@book)%>
<% end %>